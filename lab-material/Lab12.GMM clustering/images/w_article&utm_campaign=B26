<script>

  
  
    
    
  

  //old ga
  // window.dataLayer = window.dataLayer || [];
  // function gtag(){dataLayer.push(arguments);}
  // gtag('js', new Date());

  // gtag('config', 'UA-67190105-1');

</script>
<script>
$(document).ready(function() {
  $.ajax({
    url: "/notifications/api/unread_count/",
    dataType: 'json',
    type: "GET",
    success: function(data) {
      $('#notification_count').html(data.unread_count)
    },
    error: function(err) {
      console.log(err)
    }
    });
    console.log(window.location.pathname == "/notifications/unread/")
    if(window.location.pathname == "/notifications/unread/") {
      console.log("inside pathname")
      ("/notifications/unread/");
    }
    
  $("#search_contest_id p").remove();
  var index = -1;
   $('#search-contest-input-id').keydown(function(e) {
    var pressed=false;
    if($('#search-contest-input-id').val()==''){
      index = -1;
      $("#search_contest_id p").remove();
      $(this).removeClass('focus-active');
    }
    if(event.keyCode==38){
    // if up arrow key is pressed then decrease the index value.
      index = index - 1;
      if(index<0){
        //if handles the overflow
        index=0
      }
      pressed=true
    }
    else if(event.keyCode==40){
      //if down arrow key is pressed then increase the index value.
      index = index + 1

      if(index > $('#search_contest_id p').length-1){
        // it handles the overflow
        index=0
      }
      pressed=true
    }
    if(pressed==true){
      /* if any arrow key is pressed then set the value of
        that p to the input box... focus is handelled by 'active-search' class
      */
      $('#search_contest_id p').removeClass('active-search')
      $('#search_contest_id p').eq(index).addClass('active-search')
      $('#search-contest-input-id').val($('#search_contest_id p').eq(index).text())
    }
    if(event.keyCode==9){
      // if tab is pressed then close the dropdown
      index = -1
      $('#search_contest_id p').remove()
      $('#search-contest-input-id').removeClass('focus-active');
    }
  });

  $('#search-contest-input-id').keyup(function(event) {
    /*when ever any key is pressed this event got fired*/
    if($('#search-contest-input-id').val()==''){
      //if the input tag is empty then remove the focus form it and do not open the suggestor
      $("#search_contest_id p").remove();
      $(this).removeClass('focus-active');
    }
    var contest_name = $(this).val();
    // no ajax for space(32), backspace(8), delete, down-arrow,up-arrow, Enter
    if(contest_name.length > 0 && event.keyCode != 40 && event.keyCode != 38 && event.keyCode != 32 && event.keyCode !=8 && event.keyCode!=13){
      $.ajax({
        type: "POST",
        url: "/contest/searched-contest",
        data: {'contest_name': contest_name, csrfmiddlewaretoken:$('#search input[name=csrfmiddlewaretoken]').val()},
        success:function(response){
          $("#search_contest_id").empty();
          var len = response['contests'].length
          if (len==0){
            // if length is 0 then close the dropdown css
            $('#search-contest-input-id').removeClass('focus-active');
          }
          else if(len!=0 && event.keyCode==8){
            //if length not equal to 0 and backspace is pressed
            $('#search-contest-input-id').addClass('focus-active');

          }
          else{
            $('#search-contest-input-id').addClass('focus-active');
            $('#search-contest-input-id').focus();
            // $('#search_contest_id').css('width', '50%');


          }
          for (var i=0;i<response['contests'].length;i++){
            $("<p>"+response['contests'][i]+"</p>").appendTo("#search_contest_id");
          }
          $("#search_contest_id p").bind("click",function(){
            $("#search-contest-input-id").val($(this).text()).focus()
            $("#search_contest_id").empty();
            $('#search-contest-input-id').removeClass('focus-active');
            $('#sbtn').click();
          });
        }
      });
    }
    if(event.keyCode==13) {
        $('#sbtn').click();
      }
  });
  $("#sbtn").on("click", function(e){
    var search = $("#search-contest-input-id").val();
    var data = {'contest_name': search};
    $.ajax({
        type: "POST",
        url: "/contest/contest-slug",
        data: data,
        success:function(response){
          if (response['contest_slug']) {
            var url_link = "/contest/"+response['contest_slug']+"/";
            $("#search").attr("action",url_link);
            window.location.href = url_link;
          }
          else {
          console.log("Error no such contest found")
          }
        },
        error:function(err){
          console.log("error");
        }
    });
  });
  $('#sbtn-id').click(function(e) {
    $("#searchlink").toggleClass('open');
  });
  $('#close').click(function(e) {
    $("#searchlink").removeClass('open');
  });


});

// Esc click to close form
$(document).keydown(function(e) {
  if (e.keyCode == 27) {
    $("#searchlink").removeClass('open');
  }
});
</script>
