<script src="https://cdn.jsdelivr.net/npm/vue@2.6.10/dist/vue.js"></script>
<script>
  var app = new Vue({
    el: '#app',
    components: {
      'closecard': {
        props: {
        'closedItem': Object,
        },
        methods: {
        },
        filters: {
          //This is used for time the conent and title
          trimTitle: function(value, length) {
          return value.slice(0, length);
          },
          moment: function(date) {
            var date = moment(date);
            var dateString = date.format('MMM D, h:mm A');
            return dateString+" IST";

          }
        },
        template: `
        <div class="col-lg-6 col-md-6 col-sm-12 position-relative mb-3">
            <div class="av-left-thumb">
              <div class="card">
                <a :href="closedItem.participate_url" >
                  <img loading='lazy' :src="closedItem.contest_icon.thumbnail">
                  <div class="card-body">
                    <h6 class="courses-name">{{ closedItem.contest_name }}</h6>
                    <p><i class="far fa-calendar-alt"></i>{{closedItem.end_date_time | moment }}</p>
                    <span v-if="closedItem.contest_prizes.length > 27" v-html="closedItem.contest_prizes.slice(0,27)+'...'"><i class="fas fa-trophy"></i></span>
                    <span v-else v-html="closedItem.contest_prizes"><i class="fas fa-trophy"></i></span>
                    <span><i class="fas fa-user"></i>{{closedItem.registration_count}}</span>
                  </div>
                </a>
              </div>
          </div>
        </div>`
      },

    },
    data () {
      return {
        closedItems: [],
        selectedContest: '',
        currentClosedPage: 1,
        closedHasNext: true,
        num_closed: 0,
      }
    },
    mounted(){
      this.getFilteredContest('');
    },
    methods:{
      getFilteredContest: function(event) {
        this.closedItems = [];
        this.currentClosedPage = 1;
        this.contestUpdate(event, "closed");
      },
      getFilteredClosedContest: function(event) {
        this.currentClosedPage += 1;
        this.contestUpdate(event, "closed", this.currentClosedPage);
        var loader = document.getElementById('closedLoadingIconDiv')
        loader.style.display = "block";
        let showMoreBtn = document.getElementById('showMoreBtnClosed');
        if (showMoreBtn) {
          showMoreBtn.style.display = "none"
        }
      },
      contestUpdate: function(event, contestStatus, page=1) {
        vuethis = this;
        var onlyFeatured = false;
        if (event) {
          var purpose = event.target.dataset.contestfilters;
          if(purpose == 'featured') {
            onlyFeatured = true;
          } else {
            onlyFeatured = false;
          }
        }
        else {
          var purpose = "all";
        }

        vuethis.selectedContest = purpose;
        $.ajax({
          cache: false,
          url: "/contest/contest_all_filtered",
          data: {
            'page': page,
            'only_featured': onlyFeatured,
            'purpose': 'blogathon',

          },
          success: function (data) {
            if(data["contests"].length > 0) {
                for(var i=0; i < data["contests"].length; i++){
                  let con = data["contests"][i];
                  vuethis.closedItems.push(con);
                }
              }
              vuethis.closedHasNext = data["has_next"];
              var element = document.getElementById('closedLoadingIconDiv')
              element.style.display = 'none';
              let showMoreBtn = document.getElementById('showMoreBtnClosed');
              if (showMoreBtn) {
                showMoreBtn.style.display = "block"
              }
              vuethis.num_closed = data["num_contests"];
          },
          error: function(data) {
            console.log("Error with data")
            console.log(data)

          }
        });
      },
    },
  })

</script>
<script src="/static/js/jquery.waypoints.min.7d05f92297de.js"></script>
<script src="/static/js/infinite.min.a9e9aa085994.js"></script>
