<script>

  function submitFeedback(obj) {
    var avTextField = $('#avFeedbackField').val();
    if(avTextField == "") {
     $('#errorMessage').html('Please enter a description.')

    }
    else
    {
      let url = "/post_feedback/"
      var data = {
        "comment": avTextField
      };


      $.ajax({
           url: url,
           type: 'POST',
           data: JSON.stringify(data),
           dataType: "json",
           contentType: 'application/json',
           success: function(data) {
              if (data["success"]) {
                $('#errorMessage').html('')
                $('#successMessage').html('Thanks for your valuable feedback. It will help us make the platform bug-free and more useful for you and other users in the community.')
                $('#avFeedbackField').val('').empty();
              }

           },
           error : function(err){
             if (data["error"]) {
                $('#errorMessage').html('Please enter a description.')
                $('#successMessage').html('')
             }
           }

       });
     }

  }

</script>

<script>
  function set_cookie(){        
    localStorage.setItem("cookie_consent_datahack", true);
    $("#Cookies").css("display", "none");
  }

  function close_cookie(){        
    $("#Cookies").css("display", "none");
  }
</script>
