<script>
 $(document).ready(function() {
  $("#users_list_share p").remove();
  var index = -1;
  $('#share_with_others').keydown(function(e){
    var pressed=false
    if($('#share_with_others').val()==''){
      $("#users_list_share p").remove();
      $(this).removeClass('focus-active');
    }
    if(e.keyCode==38){
      // if up arrow key is pressed then decrease the index value.
      index--;
      if(index<0){
        //if handles the overflow
        index=0
      }
      pressed=true
    }
    else if(e.keyCode==40){
      //if down arrow key is pressed then increase the index value.
      index++
      if(index > $('#users_list_share p').length-1){
        // it handles the overflow
        index=0
      }
      pressed=true
    }
    if(pressed==true){
      /* if any arrow key is pressed then set the value of
        that div to the input box... focus is handelled by 'active-search' class
       */
      $('#users_list_share p').removeClass('active-search')
      $('#users_list_share p').eq(index).addClass('active-search')
      $('#share_with_others').val($('#users_list_share p').eq(index).text())
    }
    if(e.keyCode==9){
      // if tab is pressed then close the dropdown
      $('#users_list_share p').remove()
      $('#share_with_others').removeClass('focus-active');
    }
  });
  $('#share_with_others').keyup(function(e){
      /*when ever any key is pressed this event got fired*/
      if($('#share_with_others').val()==''){
        //if the input tag is empty then remove the focus form it and do not open the suggestor
        $("#users_list_share p").remove();
        $(this).removeClass('focus-active');
      }
      var user_name = $(this).val();
      // no ajax for space(32), backspace(8), delete, down-arrow,up-arrow
      if(user_name.length > 0 && e.keyCode != 40 && e.keyCode != 38 && e.keyCode != 32 && e.keyCode !=8){
        $.ajax({
          type: "POST",
          url: "/contest/data-science-blogathon-26/search_mates/",
          data: {user_name: user_name},
          success:function(response){
           $("#users_list_share").empty();
           var len = response['users'].length
            if (len==0){
              // if length is 0 then close the dropdown css
              $('#share_with_others').removeClass('focus-active');
            }
            else if(len!=0 && e.keyCode==8){
              //if length not equal to 0 and backspace is pressed
              $('#share_with_others').addClass('focus-active');
            }
            else{
              $('#share_with_others').addClass('focus-active');
              $('#share_with_others').focus()

            }
            for (var i=0;i<response['users'].length;i++){
              $("<p>"+response['users'][i].av_discuss_user_id+"</p>").appendTo("#users_list_share");
            }
            $("#users_list_share p").bind("click",function(){
              $("#share_with_others").val($(this).text()).focus()
              $("#users_list_share").empty();
              $('#share_with_others').removeClass('focus-active');
            });
          }
        });
        if(event.keyCode==13) {
        $('#share_contest_button').click();
      }
      }
  });
    $("#slack_invite_av_btn").on("click", function(){
      console.log("slack invite button was clicked")
      $('#av-slack-preloader').css("display", "block")
      $('#slack_invite_av_btn').css("display", "none")
      $('#warningModal').modal('hide');
      var data = {"email":"", "first_name":"", "csrfmiddlewaretoken":"BcUu3nldyhHp0rkJmwXU8etfeTdZbLuJqHwQUtM4rOWa98uqnXDwaFMHX4fDCmPD"};
      $.ajax({
        type : "POST",
        url : "/slack",
        data : data,
        success : function(data)
        {
          $('#av-slack-preloader').css("display", "none")
          $('#slack_invite_av_btn').css("display", "block")
          if (data["data"]["ok"])
          {
            $("#modal-body1").html("You have been successfully invited. Please check your inbox for slack invite");
            $("#modal-header1").html("Success");
            $('#myModal1').modal('show');
          }
          else{
            console.log("error was ", data["data"]);
            if (data["data"]["ok"] == false) {
              $("#modal-body").html(data["data"]["error"]);
            } else {
              $("#modal-body").html("Some error occurred. Please try again later");
            }

            $('#myModal').modal('show');
          }
        }
      });
    });

    $("#warning_message_slack").click(function(){
        $("#warning-modal-body").html("We cannot ensure your anonymity on slack. click to proceed anyways.");
        $("#warning-modal-header").html("Warning");
        $('#warningModal').modal('show');
    });
    $("#warning_message_share_challenge").click(function(){
        $("#warning-modal-body").html("You cannot challenge a friend as you are anonymous user. click to proceed anyways.");
        $("#warning-modal-header").html("Warning");
        $('#warningModal').modal('show');
    });
  });
$("#share_contest_button").click(function(){
  var user_name = $("#share_with_others").val();
  $("#share-contest-modal").modal("hide");
  $.ajax({
    type: "POST",
    data: {user_name: user_name},
    url: "/contest/data-science-blogathon-26/share/",
    success:function(response){
      console.log("in success share");
      // alert("Mail sent successfully")
      if(response['requested_user_status'] == 0) {
        alert("No such user found");
      }
    },
    error:function(err){
      console.log("in error share");
    }
  });
  // var u = ''
  // console.log('u', u)
})
 </script>
