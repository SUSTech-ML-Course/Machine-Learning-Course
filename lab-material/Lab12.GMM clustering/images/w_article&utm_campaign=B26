<script>
var blogathondiscussSocket = ''
$(document).ready(function(){
  
  
  // store the currently selected tab in the hash value
  $("ul.nav-tabs > li > a").on("shown.bs.tab", function(e) {
    var id = $(e.target).attr("href").substr(1);
    window.location.hash = id;
  });
  // on load of the page: switch to the currently selected tab
  var activeTab = window.location.hash;
  if(activeTab=="#LeaderBoard"){
      $('#LeaderBoard-tab').trigger("click");
      $('#myTab a[href="' + activeTab + '"]').tab('show');
  } else if (activeTab == "#RulesTab") {
    $('#Rules-tab').trigger("click");
    $('#myTab a[href="' + activeTab + '"]').tab('show');
  }else if (activeTab == "#DiscussTab") {
    $('#Discuss-tab').trigger("click");
    $('#myTab a[href="' + activeTab + '"]').tab('show');
  }else if(activeTab == "#TimelinesTab"){
    $('#Timelines-tab').trigger("click");
    $('#myTab a[href="' + activeTab + '"]').tab('show');
  }else if(activeTab == "#RewardsTab"){
    $('#Rewards-tab').trigger("click");
    $('#myTab a[href="' + activeTab + '"]').tab('show');
  }else if(activeTab == "#WinnerTab"){
    $('#Winner-tab').trigger("click");
    $('#myTab a[href="' + activeTab + '"]').tab('show');
  }
  else {
    $('#myTab a[href="' + activeTab + '"]').tab('show');
  }
  if($("#terms_and_cond")){
    $("#terms_and_cond").modal("show");
  }
  // modal to display similar contests
  
    $('#similarcontests').modal("hide");
  

});

