<section class="av-contest-tabs" id="app">

<div class="av-contest-content-row">
<div class="container">
<div class="tab-content withintab-contentes" id="contestTabContent">
<div class="tab-pane fade show active" id="alltabs" role="tabpanel" aria-labelledby="alltabs-tab">
<div class="av-card-with-hover events-card">

<div class="row" id="closed" v-if="closedItems[0]">
<h2 class="heading-with-bottom-line">Closed</h2>
<closecard v-for="item in closedItems" v-bind:closed-item="item" v-if="item.status == 'closed' " v-bind:key="item.contest_id"></closecard>
</div>
<div class="spinner-border text-primary d-tabel mx-auto mt-3 mb-3" role="status" id="closedLoadingIconDiv" style="display:none;">
<span class="sr-only">Loading...</span>
</div>
<button type="button" class="btn av-btn-red px-3 py-3" v-on:click="getFilteredClosedContest" v-if="closedHasNext == true" id="showMoreBtnClosed">Show more...</button>
</div>
</div>
</div>
</div>
